{
  "name": "kleinanzeigen-api",
  "version": "1.0.0",
  "description": "Kleinanzeigen API with Chrome automation",
  "main": "dist/server/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server/server.js",
    "dev": "ts-node src/server/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server/server.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
